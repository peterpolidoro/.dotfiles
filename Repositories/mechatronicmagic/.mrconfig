[mechatronicmagic-logo]
checkout =
	git clone 'git@github.com:mechatronicmagic/mechatronicmagic-logo.git' 'mechatronicmagic-logo'
	cd mechatronicmagic-logo

[mechatronicmagic-org]
checkout =
	git clone 'git@github.com:mechatronicmagic/mechatronicmagic-org.git' 'mechatronicmagic-org'
	cd mechatronicmagic-org

[farseer-pcb]
checkout =
	git clone 'git@github.com:mechatronicmagic/farseer-pcb.git' 'farseer-pcb'
	cd farseer-pcb
	git remote add public https://github.com/mechatronicmagic/farseer-pcb.git
	git remote add peterpolidoro git@github.com:peterpolidoro/farseer-pcb.git

[thruster]
checkout =
	git clone 'git@github.com:mechatronicmagic/thruster.git' 'thruster'
	cd thruster
	git remote add public https://github.com/mechatronicmagic/thruster.git
